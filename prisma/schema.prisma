generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}


model Saved {
  id      String @id @default(uuid())
  brandId Int
  userId  String
  Brand   nc_1o1g___brand_master_dev  @relation(fields: [brandId], references: [id], onDelete: Cascade)
}

model User {
  id               String   @id @default(uuid())
  userId           String
  stripeCustomerId String?
  isActive         Boolean  @default(false)
  createdAt        DateTime @default(now())
  updatedAt        DateTime
}

model UserSubscription {
  id                        String    @id @default(uuid())
  userId                    String    @unique
  stripe_customer_id        String?   @unique
  stripe_subscription_id    String?   @unique
  stripe_price_id           String?
  stripe_current_period_end DateTime?
}

model nc_1o1g___brand_master_dev {
  id                Int       @id @default(autoincrement())
  brand_id          String?   @db.VarChar
  created_at        DateTime? @default(now()) @db.Timestamp(6)
  updated_at        DateTime? @default(now()) @db.Timestamp(6)
  name              String?   @db.VarChar
  url               String?   @db.VarChar
  recent_ads        Json?
  brand_description String?
  brand_size        String?   @db.VarChar
  category_main     String?   @db.VarChar
  category_sublevel Json?
  data_source_url   String?   @db.VarChar
  metadata          Json?
  source_id         String?   @db.VarChar
  saved             Saved[]
}
